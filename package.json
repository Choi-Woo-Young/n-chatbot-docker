{
  "name": "n-chatbot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "source env/bin/activate && pip install -r api/requirements.txt && FAST_API_MODE=development python -m uvicorn api.index:app --reload --host 0.0.0.0 --port 8019",
    "fastapi-test": "source env/bin/activate && pip install -r api/requirements.txt && FAST_API_MODE=test python -m uvicorn api.index:app --reload --host 0.0.0.0 --port 8019",
    "next-dev": "cross-env PORT=3019 next dev",
    "next-test": "cross-env NODE_ENV=test PORT=3019 next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "test": "concurrently \"npm run next-test\" \"npm run fastapi-test\"",
    "build": "next build",
    "build:test": "cross-env NODE_ENV=test dotenv -e .env.test -- next build",
    "start": "cross-env PORT=3019 next start",
    "lint": "next lint"
  },
  "dependencies": {
